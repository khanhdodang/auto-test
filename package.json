{
  "name": "auto-test",
  "version": "0.0.1",
  "description": "Automation testing script using Mocha with ES6 and Babel 6",
  "scripts": {
    "test": "mocha --compilers js:babel-core/register --timeout 600000 src/*.js",
    "test-android-web": "mocha --compilers js:babel-core/register --timeout 600000 src/test-android-web.js",
    "test-ios-web": "mocha --compilers js:babel-core/register --timeout 600000 src/test-ios-web.js",
    "test-android-app": "mocha --compilers js:babel-core/register --timeout 600000 src/test-android-app.js",
    "test-ios-app": "mocha --compilers js:babel-core/register --timeout 600000 src/test-ios-app.js"
  },
  "keywords": [
    "automation",
    "js",
    "es6",
    "mocha",
    "appium",
    "chai",
    "babel"
  ],
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-flow-strip-types": "^6.14.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.14.0",
    "bluebird": "^3.4.6",
    "chai": "^3.5.0",
    "mocha": "^3.0.2",
    "colors": "^1.0.3",
    "wd": "^1.1.1"
  },
  "dependencies": {
    "babel-polyfill": "^6.13.0",
    "wd": "^1.1.3"
  }
}
